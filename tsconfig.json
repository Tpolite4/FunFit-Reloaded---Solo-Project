{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "outDir": "./build",
    "jsx": "react-jsx",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "include": ["./src/**/*", "typings.d.ts"],
  "exclude": ["node_modules"]
}
